{
  "compilerOptions": {
    "module": "esnext",
    // "esModuleInterop": true,
    // "allowSyntheticDefaultImports": true,
    "target": "es2019",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "alwaysStrict": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "declaration": true,
    "declarationDir": "types",
    "outDir": "lib",
  },
  "include": [
    "src/**/*"
  ]
}